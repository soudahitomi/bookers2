<div class= "container" >
  <div class= "row">
    <div class="col-md-3"><!--col-md-3の開始タグ-->

<%= render partial: 'list', locals: { user: @user, book: @book, } %>

    </div><!--col-md-3の閉じタグ-->

    <div class="col-md-8 offset-md-1">
      <h1>Users</h1>
      <table class="table table-striped">
        <tbody>
          <% @users.each do |user| %>

          <tr>
            <td><%= image_tag user.get_profile_image(100,100) %></td>
            <td><%= user.name %></td>
            <td><%= link_to "show", user_path(user.id) %></td>
          </tr>
          <% end %>
          </tbody>
      </table>
    </div><!--col-md-8 offsetの閉じタグ-->
  </div><!--rowの閉じタグ-->
</div><!--contanerの閉じタグ-->